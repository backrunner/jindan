{
  "name": "jindan",
  "version": "0.1.0",
  "description": "A solution which can give sites a long-term high availability",
  "main": "dist/jindan.js",
  "module": "dist/jindan.esm.js",
  "typings": "dist/types/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "cross-env NODE_ENV=dev rollup -c rollup.config.ts -w",
    "build": "rimraf ./dist && npm run build:bundle && npm run build:module",
    "build:bundle": "cross-env NODE_ENV=production BUILD_TARGET=bundle rollup -c rollup.config.ts",
    "build:module": "cross-env NODE_ENV=production BUILD_TARGET=module rollup -c rollup.config.ts"
  },
  "author": "BackRunner",
  "license": "Apache-2.0",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.11.56",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "eslint-config-alloy": "^4.6.2",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.33.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@aws-crypto/sha256-browser": "^2.0.1",
    "dexie": "^3.2.2",
    "lodash-es": "^4.17.21"
  }
}
